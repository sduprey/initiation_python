    "        k = k+1\n",
    "        plt.subplot(pmax,pmax,k)\n",
    "        if i==j:\n",
    "            plt.hist(X0[:,i],6)\n",
    "            plt.axis('tight')\n",
    "        else:\n",
    "            plt.plot(X0[:,j],X0[:,i], '.')\n",
    "        plt.axis('tight')\n",
    "        if i==1:\n",
    "            plt.title(class_names[0][j][0])\n",
    "        plt.tick_params(axis='x', labelbottom='off')\n",
    "        plt.tick_params(axis='y', labelleft='off')"
   ]
